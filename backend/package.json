{
  "name": "mad-sales-portal-backend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "watch": "yarn tsc -w",
    "dev": "nodemon dist/index.js",
    "start": "ts-node src/index.ts",
    "start-dev": "ts-node-dev --respawn --watch --transpile-only ./src/index.ts",
    "start-prod": "tsc && node ./dist/index.js",
    "migrate": "sequelize-cli db:migrate",
    "migration:generate": "sequelize-cli migration:generate --name auto",
    "migration:create": "sequelize-cli migration:create --name auto",
    "seed": "sequelize-cli db:seed:all",
    "seed:generate": "sequelize-cli seed:generate --name auto",
    "seed:create": "sequelize-cli seed:create --name auto"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.387.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.3",
    "pg": "^8.11.0",
    "randomstring": "^1.3.0",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.6",
    "@types/nodemailer": "^6.4.9",
    "@types/randomstring": "^1.1.8",
    "babel-plugin-module-resolver": "^5.0.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
